.nav-wrap{
  width: 100%;
  overflow: hidden;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999; 
}

.nav-section{
  position: absolute;
  left: 0;
  z-index: 10;
  width: 100%;
}

.nav-section .back{
  width: 34rpx;
  height: 34rpx;
  position: absolute;
  top: 50%;
  left: 0;
  transform: translateY(-50%);
  padding: 20rpx;
}

.nav-section .nav-title {
  font-size: 34rpx;
  /* color: #000000; */
  font-weight:500;
}

.nav-search_show {
  position: absolute;
  font-size: 0;
  top: 50%;
  transform: translateY(-50%);
}

.nav-search {
  width: 100%;
  min-height: none;
  border-radius: 32rpx;
  border: none;
  background: #f3f4f5;
}

.nav-search_title {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  vertical-align: middle;
  /* font-size: 26rpx; */
  color: #B2B9C8;
}

.icon-search {
  width: 36rpx;
  height: 36rpx;
  /* position: relative;
  top: 4rpx; */
  margin-right: 12rpx;
  vertical-align: middle;
}
.icon-search_text {
  display: inline-block;
  vertical-align: middle;
  font-size: 26rpx;
}

.nav-search_use .nav-search {
  font-size: 26rpx;
  color: #B2B9C8;
  padding: 0 60rpx;
  box-sizing: border-box;
  color: #0A1735;
}

.nav-search_use .icon-search {
  position: absolute;
  width: 36rpx;
  height: 36rpx;
  top: 50%;
  left: 20rpx;
  transform: translateY(-50%);
}
.search-placeholder {
  color: #B2B9C8;
}

.icon-search_del {
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  top: 50%;
  right: 20rpx;
  transform: translateY(-50%);
}